#
# occampi-newsyn.ldef - language definitions for a new occam-pi syntax (experimenting)
# identifiers must start at the left-margin, indentation is line-continuation
#

.IDENT		"occampi-newsyn"
.DESC		"occam-pi parallel programming language, http://occam-pi.org/">
.MAINTAINER	"Fred Barnes <frmb@kent.ac.uk>"


.SECTION "occampi-newsyn"

.GRULE	"opi:nullreduce" "N+R-"
.GRULE	"opi:nullpush" "0N-"
.GRULE	"opi:nullset" "0R-"


.SECTION "occampi-newsyn-postprod"


